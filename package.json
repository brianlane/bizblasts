{
  "name": "app",
  "private": "true",
  "scripts": {
    "build:css:application": "sass ./app/assets/stylesheets/application.sass.scss:./app/assets/builds/application.css --no-source-map --load-path=node_modules",
    "build:css:active_admin": "sass ./app/assets/stylesheets/active_admin.scss:./app/assets/builds/active_admin.css --no-source-map --load-path=node_modules",
    "build:css": "npm run build:css:application && npm run build:css:active_admin"
  },
  "devDependencies": {
    "sass": "^1.86.3"
  }
}
