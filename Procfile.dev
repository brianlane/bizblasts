web: env RUBY_DEBUG_OPEN=true bin/rails server
css: bundle exec tailwindcss --watch -i ./app/assets/tailwind/application.css -o ./app/assets/builds/tailwind.css --cwd .
js: /bin/bash -lc "bun build ./app/javascript/application.js --outdir ./app/assets/builds --watch --no-clear-screen"
worker: bin/jobs